<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/categoryButton"
        style="@style/smallMontserrat_Button"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dimen_21_dp"
        android:background="@drawable/background_filter_unfocused"
        android:paddingHorizontal="@dimen/dimen_12_dp"
        android:paddingVertical="@dimen/dimen_4_dp"
        android:layout_marginEnd="@dimen/dimen_4_dp"
        android:text="@string/top_rated"
        android:textAlignment="center"
        android:textAllCaps="false"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>